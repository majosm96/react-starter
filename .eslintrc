{
    "parser": "babel-eslint",
    "extends": ["eslint:recommended","airbnb"],
    "env": {
        "browser": true
    },
    "rules": {
        "react/prefer-stateless-function": 0,
        "react/jsx-filename-extension": 0,
        "linebreak-style": 0,
        "react/forbid-prop-types": 0,
        "no-console": 0
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ]
}